<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.bryan.a52sensores_acelerometro.MainActivity">

    <TextView
        android:id="@+id/ejex"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="38dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/ejez"
        app:layout_constraintStart_toStartOf="@+id/ejez"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/ejey"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/ejex"
        app:layout_constraintTop_toBottomOf="@+id/ejex" />

    <TextView
        android:id="@+id/ejez"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="67dp"
        android:layout_marginTop="48dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ejey" />
</android.support.constraint.ConstraintLayout>
